name = 'pasta-web'
bind = '0.0.0.0:8000'

debug = False
reload = False
daemonize = False

chdir = r'/app'

workers = 1
threads = 8
worker_class = 'sync'
worker_connections = 128

timeout = 60
keep_alive = 30

access_logfile = r'/var/log/pasta/access.log'
error_logfile = r'/var/log/pasta/access-errors.log'

